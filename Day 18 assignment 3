#include <stdio.h> 

main() {
  int n, i, c = 0;
  printf("Enter any number n:");
  scanf("%d", &n);

  //logic
  for (i = 1; i <= n; i++) {
      if (n % i == 0) {
         c++;
      }
  }

  if (c == 2) {
        printf("Prime number");
  }
  else {
         printf("not a Prime number");
  }
  return 0;    
}
Output:
1) 15
Prime number
2) 5
not a prime number
